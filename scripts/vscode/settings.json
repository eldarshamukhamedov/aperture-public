{
  "files.exclude": {
    "**/*.js.snap": true,
    "**/build": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/yarn-error.log": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.workingDirectories": [
    { "pattern": "./apps/*" },
    { "pattern": "./modules/*" }
  ],
  "typescript.tsdk": "node_modules/typescript/lib"
}
